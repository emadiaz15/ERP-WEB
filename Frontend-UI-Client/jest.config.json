{
  "testEnvironment": "jsdom",
  "setupFiles": ["<rootDir>/jest.setup.js"],
  "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"],
  "moduleNameMapper": {
    "^@/utils/getApiBaseUrl$": "<rootDir>/src/utils/getApiBaseUrl.node.js",
    "^@/features/notifications/hooks/wsAuthMode$": "<rootDir>/src/features/notifications/hooks/node/wsAuthMode.js",
    "^@/features/notifications/hooks/wsEnv$": "<rootDir>/src/features/notifications/hooks/wsEnv.node.js",
    "^@/features/notifications/env$": "<rootDir>/src/features/notifications/node/env.js",
    "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}
