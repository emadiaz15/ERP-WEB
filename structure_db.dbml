//////////////////////////////////////////////////////////////
//   DICCIONARIO DE ABREVIACIONES Y SINÓNIMOS (NUEVO)
//////////////////////////////////////////////////////////////

Table articulo_abreviaciones {
  abb_id int [pk, not null, increment]
  art_codi int [not null, ref: > articulos.art_codi]
  abreviacion varchar(50) [not null]
  palabra_completa varchar(255) [not null]
  peso int [default: 1]
  notas text
  activo int [default: 1]

  Indexes {
    (abreviacion)
    (art_codi)
  }
}

Table articulo_sinonimos {
  sin_id int [pk, not null, increment]
  art_codi int [not null, ref: > articulos.art_codi]
  sinonimo varchar(255) [not null]
  tipo varchar(50) [default: 'cliente'] // cliente / interno / tecnico
  activo int [default: 1]

  Indexes {
    (sinonimo)
    (art_codi)
  }
}

Table diccionario_terminos {
  term_id int [pk, not null, increment]
  termino varchar(255) [not null]
  tipo varchar(50) [default: null] // material / medida / unidad / accion
  valor varchar(255) [default: null]
  notas text
}

//////////////////////////////////////////////////////////////
//   TABLAS IA PARA PRESUPUESTOS INTELIGENTES
//////////////////////////////////////////////////////////////

Table ia_presupuestos_meta {
  ia_presu_id int [pk, not null, increment]
  presup_codi int [ref: > presupuestos.presup_codi]
  cli_codi int [ref: > clientes.cli_codi]
  whatsapp_number varchar(30) [default: null]
  canal varchar(20) [default: 'WHATSAPP']
  raw_text text
  origen_archivo varchar(255) [default: null]
  ia_version varchar(50) [default: 'v1']
  confianza_global decimal(5,2) [default: 0.00]
  estado_revision varchar(15) [default: 'PENDIENTE']

  created_at datetime [default: `CURRENT_TIMESTAMP`]
  updated_at datetime [default: `CURRENT_TIMESTAMP`]
}

Table ia_matching_log {
  match_id int [pk, not null, increment]
  ia_presu_id int [ref: > ia_presupuestos_meta.ia_presu_id]
  linea_original text
  cantidad_solicitada decimal(15,3) [default: 1]

  art_codi_sugerido int [ref: > articulos.art_codi]
  confianza_match decimal(5,2) [default: 0.00]
  estado varchar(15) [default: 'AUTO'] // AUTO / MANUAL / REVISADO

  art_codi_final int [ref: > articulos.art_codi]
  nota_vendedor text

  created_at datetime [default: `CURRENT_TIMESTAMP`]
}

Table ia_alias_articulos {
  alias_id int [pk, not null, increment]
  art_codi int [ref: > articulos.art_codi]
  alias_text varchar(255)

  fuente varchar(20) [default: 'IA'] // IA / CLIENTE / SISTEMA
  cli_codi int [ref: > clientes.cli_codi]
  region varchar(100)
  confianza decimal(5,2) [default: 100.00]

  created_at datetime [default: `CURRENT_TIMESTAMP`]
  created_by varchar(50) [default: 'IA']
}

Table pedidos_preparacion {
  prep_codi int [pk, not null, increment]         // ID interno del proceso de preparación
  ped_codi int [not null, ref: > pedidos.ped_codi]   // Pedido asociado
  cli_codi int [not null, ref: > clientes.cli_codi] // Cliente (copia del pedido para performance)
  fecha_creado datetime [not null]                // Fecha y hora en que nació el pedido
  usu_codi int [ref: > usuarios.usu_codi]         // Usuario asignado a preparar
  estado int [not null, default: '0'] // pendiente / en_proceso / completado
  tran_codi int                                   // Nombre transporte o método
  cant_items int [default: 0]                     // Cache: Cantidad de ítems del pedido
  observaciones text                              // Notas internas del área de preparación
} 

Ref: "transportes"."tran_codi" < "pedidos_preparacion"."tran_codi"

Table notificaciones {
  noti_id        int [pk, not null, increment]   // ID autoincremental
  titulo         varchar(100)                    // Título visible del mensaje
  mensaje        varchar(255)                    // Contenido principal
  tipo           varchar(20) [default: 'info']   // info | warning | error | success
  prioridad      tinyint [default: 1]            // 1 baja | 2 media | 3 alta
  leida          tinyint [default: 0]            // 0 no leída | 1 leída
  fija           tinyint [default: 0]            // Si es fija, no se autocierra
  expira         tinyint [default: 0]            // Si expira automáticamente
  fecha_expira   datetime                        // Cuándo expira
  fecha_crea     datetime [default: `CURRENT_TIMESTAMP`] // Marca temporal de creación
  usu_codi       varchar(30)                     // Usuario destinatario (VFP)
  modulo_accion  varchar(30)                     // Módulo de origen (PEDIDOS, FACTURAS)
  param_accion   varchar(100)                    // Datos para acción: ej PEDI_CODI=1234                    // Nombre de sonido asociado
}

Ref: "usuarios"."usu_codi" < "notificaciones"."usu_codi"