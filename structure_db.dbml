Table insumos {
  insu_codi int [pk]
  insu_desc varchar(45)
  insu_unimed varchar(2)
  insu_stock double(15,3)
  insu_stmin double(15,3)
  insu_costo double(15,3)
  insu_compras double(15,3)
  insu_ultcpra double(15,3)
  insu_rub_id int
  insu_inrota int
  insu_activo int
  insu_detalle varchar(254)
}

Table insu_rubros {
  insu_rub_id int [pk, increment]
  insu_rub_desc varchar(45)
}

Table insu_historicostock {
  hs_codi int [pk, increment]
  insu_codi int [ref: > insumos.insu_codi]
  hs_fecha datetime
  hs_tipmov varchar(1)
  hs_anterior double(15,3)
  hs_cant double(15,3)
  hs_saldo double(15,3)
  hs_obser text
  hs_deta text 
  insu_ajuscodi int
  recep_codi int
  rem_codi int
  cond_codi int
  codi_clipro int
}

Table insu_ajustes {
  insu_ajuscodi int [pk, increment]
  insu_ajusfech date
  insu_ajusconc varchar(1)
  insu_ajusobserv text
}

Table insu_ajus_insumos {
  insu_ajuscodi int [ref: > insu_ajustes.insu_ajuscodi]
  insu_codi int [ref: > insumos.insu_codi]
  insu_ajus_cant double(11,3)
  indexes {
    (insu_ajuscodi) [pk]
  }
}

Table insu_articulos {
  insuart_codi int [pk, increment]
  insu_codi int [ref: > insumos.insu_codi]
  art_codi int [ref: > articulos.art_codi]
  cantidad decimal(15,3)
  fecha_asociacion datetime
  activo tinyint(1)
}

Table insumos_proveedores {
  insuprov_codi int [pk, increment]
  insu_codi int [ref: > insumos.insu_codi] 
  prov_codi int [ref: > proveedores.prov_codi]
  insuprov_costo double(15,3)
  insuprov_moneda enum('P','D')
  insuprov_lista int //campo bandera para marcar un proveedor de insumo activo
  insuprov_fechact date
  insuprov_observ text
  activo tinyint(1)
}

Table insu_prov_histocosto {
  hist_codi int [pk, increment]
  hist_fecha datetime
  hist_costo_anterior double(15,3)
  hist_costo_nuevo double(15,3)
  hist_moneda int(1)
  insuprov_codi int
}

Table art_metricas {
  art_codi                int        [pk, note: 'Código del artículo']
  vend_6m                 int        [note: 'Ventas últimos 6 meses']
  vend_3m                 int        [note: 'Ventas últimos 3 meses']
  vend_prev_3m            int        [note: 'Ventas de los 3 meses previos']
  tendencia_3m            int        [note: 'Vend_3m - Vend_prev_3m']
  meses_con_venta         int        [note: 'Meses con ventas en la ventana de 6m']

  rotacion                int        [note: 'Clasificación ABC (1–5)']
  rotacion_pct            decimal(12,2) [note: 'Velocidad relativa de salida']

  prom_mensual            decimal(12,2) [note: 'Promedio mensual basado en 6 meses']

  dias_ultima_venta       int        [note: 'Días desde la última venta']

  stock_actual            decimal(12,2) [note: 'Stock actual']
  stock_min               decimal(12,2) [note: 'Stock mínimo']
  posicion                decimal(12,2) [note: 'Stock real proyectado']

  comprado_pendiente      decimal(12,2) [note: 'Compras pendientes de entrega']
  pedidos_pend            decimal(12,2) [note: 'Pedidos de clientes pendientes']

  comprado_3m             decimal(12,2) [note: 'Compras últimos 3 meses']
  comprado_6m             decimal(12,2) [note: 'Compras últimos 6 meses']

  score_compra            decimal(10,2) [note: 'Score final 0–100']

  fecha_calc              datetime   [note: 'Fecha de cálculo']
}


